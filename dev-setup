echo "Kuona developer setup"

echo Initialising sub-modules

git submodule init

echo Updating repositories

git submodule update

echo Module status

git submodule status

echo Installing required tool using hombrew and bundle

brew bundle

cd dashboard; sh dev-setup; cd ..

echo Initial environment service build

cd environment-service
lein install
lein midje

cd ../git-collector
lein install
lein test
